# tbot_provisioning.service

[Unit]
Description=RIGD TradeBot Provisioning Orchestrator (Keys/Secrets Generation Phase)
After=network.target

[Service]
Type=simple
Environment=PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin
Environment="PROVISIONING_PHASE=1"
WorkingDirectory=%h/rigd_tbot
ExecStart=/usr/bin/env python3 tbot_bot/config/provisioning_runner.py
Restart=no

[Install]
WantedBy=default.target
